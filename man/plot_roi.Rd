\examples{
\dontrun{
  migob <- import_XML("file.XML") |>
           Apply_TMate_Filter() |>
           clustering(predef = "morphological")
  # plot the first three cells
  plot_roi(migob,1:3)
  # put plots off all cells into "output.pdf"
  pdf("output.pdf")
  plot_roi(migob)
  dev.off()
}}

